        "/getActiveCartForUser": {
            "post": {
                "operationId": "7c97b5eae77677c410edf87ac88d3543",
                "parameters": [
                    {
                        "name": "request",
                        "in": "header",
                        "schema": {
                            "properties": {
                                "user_id": {
                                    "type": "integer",
                                    "example": "2"
                                }
                            },
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Récupère un panier actif ou renvoie null",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "cart": {
                                        "id": "1",
                                        "user_id": "2",
                                        "status": "1"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },

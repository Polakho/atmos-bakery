    "paths": {
        "/login": {
            "post": {
                "operationId": "3538faba4efdf3b92e0320b7092c2ad9",
                "parameters": [
                    {
                        "name": "request",
                        "in": "header",
                        "schema": {
                            "properties": {
                                "mail": {
                                    "type": "string",
                                    "example": "toto@uyb.fr"
                                },
                                "password": {
                                    "type": "string",
                                    "example": "monmdp"
                                }
                            },
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Permet de vérifier les logins transmis",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "message": "Good login",
                                    "user_id": "3"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/register": {
            "post": {
                "operationId": "25003b1b0a6a37be5aa8a7ff35232b19",
                "parameters": [
                    {
                        "name": "request",
                        "in": "header",
                        "schema": {
                            "properties": {
                                "mail": {
                                    "type": "string",
                                    "example": "david@uyb.fr"
                                },
                                "password": {
                                    "type": "string",
                                    "example": "monmdp"
                                },
                                "retypePassword": {
                                    "type": "string",
                                    "example": "monmdp"
                                },
                                "name": {
                                    "type": "string",
                                    "example": "David"
                                },
                                "f_name": {
                                    "type": "string",
                                    "example": "Mouret"
                                },
                                "roles": {
                                    "type": "string",
                                    "example": "CLIENT"
                                }
                            },
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Permet de créer un user et renvoie son id",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "user_id": "9"
                                }
                            }
                        }
                    }
                }
            }
        },

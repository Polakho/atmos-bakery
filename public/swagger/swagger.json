                    {
                        "name": "request",
                        "in": "header",
                        "schema": {
                            "properties": {
                                "cart_id": {
                                    "type": "integer",
                                    "example": "2"
                                },
                                "quantity": {
                                    "type": "integer",
                                    "example": "1"
                                },
                                "product_id": {
                                    "type": "integer",
                                    "example": "34"
                                }
                            },
                            "type": "object"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Permet d'ajouter un produit en quantité (ou pas) au panier.",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "message": "bravo tu as réussis !",
                                    "contain_id": "3"
                                }
                            }
                        }
                    }
                }
            }
        },
